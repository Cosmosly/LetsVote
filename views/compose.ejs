<%- include('partials/header') %>
<h1>New Vote</h1>
<form action="/compose" method="post">
    <div class="form-group">
        <label>Title</label>
        <input class="form-control" type="text" name="compose_title" size=60% >
        <label>EntryPoint</label>
        <input class="form-control" type="text" name="compose_entryPoint" placeholder="Ex. type 'a', 'b', 'c' as voting group" size=60% >
        <label>Description</label>
        <textarea class="form-control" name="compose_text" id="" cols=60% rows="5"></textarea>
    </div>
    <button type="submit" class="btn btn-success" name="compose_submit">Publish</button>
</form>
<%- include('partials/footer') %>